{ 
    "workbench.colorTheme": "Tokyo Night Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        //"sideBar.foreground": "#f8f8f2",
    },
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.confirmOnExit": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.profiles.osx": {
       "fish": {
        "path": "/usr/local/bin/fish"
       } 
    },
    
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "terraform.languageServer": {
          "external": true,
          "args": [
            "serve"
          ]
    },

    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,

    "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; font-weight: bold; margin-left: 1cm;\"></div> <div style=\"font-size: 9px; font-weight: bold; margin-left: auto; margin-right: 1cm; \"> <span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdown-pdf.footerTemplate": "<div style=\"font-size: 9px; font-weight: bold; margin: 0 auto;\">Use, reproduction or disclosure is subject to restrictions set forth in Contract Number 2014-14031000011 and Sub Contract No. Coder Technologies 01.</div>",

    "sync.autoDownload": false,
    "sync.gist": "ericsettingssync",
    "sync.autoUpload": true,
    "sync.forceUpload": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "cSpell.ignoreWords": [
        "autofetch",
        "donot",
        "ericsettingssync",
        "git",
        "info",
        "live",
        "msg",
        "server",
        "settings",
        "show",
        "devurls",
        "proxied",
        "dotfiles",
        "envproxy",
        "kubectl",
        "envagent",
        "OIDC",
        "cemanager",
        "CVMs",
        "kubernetes",
        "sysbox",
        "sbin",
        "distro"
    ],

    "workbench.editor.enablePreview": false,
    "workbench.experimental.useCustomHover":true,
    "diffEditor.renderSideBySide": false,
    "cSpell.userWords": [
        "letsencrypt"
    ],
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontSize": 14,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "underline",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "material-icon-theme.folders.theme": "specific",
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "coder.node-js": "linux",
        "coder.react": "linux",
        "coder.rubyonrails": "linux",
        "coder.vnc": "linux",
        "coder.intellij": "linux",
        "coder.pandas": "linux",
        "ec2-18-117-255-159.us-east-2.compute.amazonaws.com": "linux",
        "coder.eric-dogfood": "linux",
        "coder.team-env": "linux",
        "coder.eric-test": "linux"
    },
    "docker.showStartPage": false,
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.mac": "/Users/eric/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "workbench.startupEditor": "none",
    "bracketPairColorizer.depreciation-notice": false,
    "workbench.productIconTheme": "bootstrap-product-icons",
    "github.gitAuthentication": false,
    "go.toolsManagement.autoUpdate": true,
    "editor.rulers": [
        80,
        120
    ],
    "files.exclude": {
        "**/.git": false
    },
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "security.workspace.trust.enabled": false,
    "window.zoomLevel": 1
}